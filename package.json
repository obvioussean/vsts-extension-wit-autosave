{
  "devDependencies": {
    "tfx-cli": "^0.5.14",
    "typescript": "^3.0.1"
  },
  "name": "vsts-extension-wit-autosave",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "tsc",
    "create-dev": "tfx extension create --manifest-globs vss-extension.json --rev-version --overrides-file configs/dev.json",
    "publish-dev": "tfx extension publish --manifest-globs vss-extension.json --overrides-file configs/dev.json",
    "create-prod": "tfx extension create --manifest-globs vss-extension.json --rev-version --overrides-file configs/release.json",
    "publish-prod": "tfx extension publish --manifest-globs vss-extension.json --overrides-file configs/release.json"
  },
  "dependencies": {
    "@types/jquery": "^3.3.5",
    "vss-web-extension-sdk": "^5.134.0"
  }
}
